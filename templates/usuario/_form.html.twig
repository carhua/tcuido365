{% import 'macro/action.html.twig' as action %}

{{ action.frm_start(form) }}

    {{ action.frm_column(
        action.frm_column_field(form.fullName),
        action.frm_column_field(form.username)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.password.first),
        action.frm_column_field(form.password.second)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.usuarioRoles),
        action.frm_column_field(form.email)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.telefono),
        action.frm_column_field(form.provincia,'','m-select2')
    )}}

  {{ action.frm_column(
      action.frm_column_field(form.distrito,'','m-select2'),
      action.frm_column_field(form.centroPoblado)
  ) }}

  {{ action.frm_column(
      action.frm_column_field(form.institucion,'','m-select2'),
      null,
      null,
      null,2
  ) }}

    {{ action.frm_column(
        action.frm_column_btn(button_label|default('Guardar')),
        null,
        null,
        null,
        2
    ) }}

{{ action.frm_end(form) }}