{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Tipo Explotacion' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'tipo_explotacion_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title,_section,'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('Nombre',tipo_explotacion.nombre) }}
    {{ action.tbl_tr('Activo',tipo_explotacion.isActive | yesno) }}
    {{ action.tbl_end() }}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_options_ico.html.twig' %}
{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'tipo_explotacion','entity': tipo_explotacion} %}
{% endblock %}

{% block _main_actions %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'tipo_explotacion','entity': tipo_explotacion} %}
{% endblock %}

{% block _javascripts %}
{% endblock %}

