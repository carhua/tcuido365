{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Provincia' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'provincia_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title,_section,'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('Nombre',provincia.nombre) }}
    {{ action.tbl_tr('Activo',provincia.isActive | yesno) }}
    {{ action.tbl_end() }}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_options_ico.html.twig' %}
{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'provincia','entity': provincia} %}
{% endblock %}

{% block _main_actions %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'provincia','entity': provincia} %}
{% endblock %}

