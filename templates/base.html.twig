<script>
    $(document).ready(function () {
        {% for label, messages in app.flashes %}
        {% for message in messages %}
        {% if label == 'success' %}
        Notify.success("{{ message | trans }}");
        {% endif %}
        {% if label == 'warning' %}
        Notify.warning("{{ message | trans }}");
        {% endif %}
        {% if label == 'danger' %}
        Notify.danger("{{ message | trans }}");
        {% endif %}
        {% endfor %}
        {% endfor %}

        // $('#frm_filters').hide();

        $(document).on('click', '#action_options', function (e) {
            e.preventDefault();
            $('#frm_filters').toggle('fast');
            return false;
        });
    });
</script>

